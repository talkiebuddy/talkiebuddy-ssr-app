{
  "name": "talkiebuddy-ssr-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:js-fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint:style-fix": "stylelint --fix **/*.{vue,css} --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "lint-fix": "npm run lint:js-fix && npm run lint:style-fix",
    "test": "jest",
    "storybook:serve": "npx nuxt storybook --static-dir static",
    "storybook:build": "npx nuxt storybook build --static-dir static",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "gh-pages-deploy": "node scripts/gh-pages-deploy.js"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/pwa": "^3.0.2",
    "core-js": "^3.6.5",
    "credit-card-type": "^9.1.0",
    "nuxt": "^3.4.3",
    "vue-tel-input": "^4.4.2",
    "vuelidate": "^0.7.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/storybook": "^3.3.1",
    "@nuxtjs/style-resources": "^1.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@storybook/addon-a11y": "^6.1.9",
    "@storybook/addons": "^6.1.9",
    "@storybook/theming": "^6.1.9",
    "@vue/test-utils": "^1.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.5.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "chalk": "latest",
    "chromatic": "^5.5.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "execa": "latest",
    "fibers": "^5.0.0",
    "html-loader": "^1.3.2",
    "husky": "^4.3.8",
    "jest": "^26.5.0",
    "lint-staged": "^10.5.3",
    "node-emoji": "^1.10.0",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "sass": "^1.32.5",
    "sass-loader": "^10.1.1",
    "storybook-addon-deps": "^2.1.3",
    "storybook-addon-designs": "^6.0.0",
    "storybook-dark-mode": "^1.0.4",
    "storybook-dep-webpack-plugin": "^1.0.7",
    "storybook-design-token": "^0.8.1",
    "stylelint": "^13.9.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "vue-jest": "^3.0.4"
  },
  "keywords": [
    "Pattern Lab",
    "Atomic Web Design",
    "Node",
    "Gulp",
    "Javascript"
  ],
  "author": "Reza Fauzi Augusdi",
  "license": "MIT"
}
